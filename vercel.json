{
    "version": 2,
    "projects": [
      {
        "src": "frontend/",
        "use": "@vercel/static"
      },
      {
        "src": "backend/",
        "use": "@vercel/node"
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/backend/index.js"
      },
      {
        "source": "/(.*)",
        "destination": "/frontend/$1"
      }
    ]
  }
  